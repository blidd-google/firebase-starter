{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "eslint:recommended",
      "google",
      "plugin:react/recommended",
      "next/core-web-vitals"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
      "object-curly-spacing": ["error", "always"],
      "react/prop-types": "off",
      "require-jsdoc": "off",
      "guard-for-in": "off",
      "indent": ["error", 2, {
        "FunctionExpression": 
        {
          "body": 1, 
          "parameters": 2
        } 
      }],
      "new-cap": "off"
  }
}
